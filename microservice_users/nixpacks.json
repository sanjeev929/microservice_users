{
    "phases": {
      "install": {
        "commands": [
          "pip install -r ../requirements.txt"
        ]
      },
      "build": {
        "commands": [
          "python manage.py collectstatic --noinput"
        ]
      },
      "start": {
        "commands": [
          "gunicorn microservice_users.wsgi"
        ]
      }
    },
    "environment": {
      "DJANGO_SETTINGS_MODULE": "microservice_users.settings"
    }
  }
  